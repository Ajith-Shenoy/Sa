<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="in.edu.reva.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".About_saviskara"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Contact_Us"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Faq"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Web_Visit"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name=".Splashscreen"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TechMain"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Recreational"
            android:screenOrientation="portrait" />
        <activity
            android:name=".CulturalMain"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Firebase"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Inhouse"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Paytm"
            android:screenOrientation="portrait" />
        <activity
            android:name=".queryWeb"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity android:name=".Venue_Time"></activity>
    </application>

</manifest>